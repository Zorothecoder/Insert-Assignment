<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Data</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-4bw+/aepP/YC94hEpVNVgiZdgIC5+VKNBQNGCHeKRQN+PtmoHDEXuppvnDJzQIu9" crossorigin="anonymous">
</head>

<body>
    <div class="container my-5">
        <div class="row">
            <div class="col border me-1">
                <div class="row bg-primary"><h3 class="text-center">Users Table</h3></div>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">User_Id</th>
                            <th scope="col">Name</th>
                            <th scope="col">Email-ID</th>
                            <th scope="col">Gender</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0;i<users.length; i++){ %>
                        <tr>
                            <td scope="row"><%- users[i].User_id %></td>
                            <td><%- users[i].Name %></td>
                            <td><%- users[i].Email_id %></td>
                            <td><%- users[i].Gender %></td>
                        </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
            <div class="col border ms-1">
                <div class="row bg-primary"><h3 class="text-center">Address Table</h3></div>
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col">Address_Id</th>
                            <th scope="col">Address</th>
                            <th scope="col">State</th>
                            <th scope="col">City</th>
                            <th scope="col">Pincode</th>
                            <th scope="col">User_id</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% for(var i=0;i<users.length; i++){ %>
                            <tr>
                                <td scope="row"><%- address[i].Address_id %></td>
                                <td><%- address[i].Address %></td>
                                <td><%- address[i].State %></td>
                                <td><%- address[i].City %></td>
                                <td><%- address[i].Pincode %></td>
                                <td><%- address[i].User_id %></td>
                            </tr>
                        <% } %>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.1/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-HwwvtgBNo3bZJJLYd8oVXjrBZt8cqVSpeBNS5n7C8IVInixGAoxmnlMuBnhbgrkm"
        crossorigin="anonymous"></script>
</body>

</html>